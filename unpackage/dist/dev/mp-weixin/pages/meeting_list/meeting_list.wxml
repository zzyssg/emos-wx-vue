<view class="page"><image class="logo" src="../../static/logo-3.jpg" mode="widthFix"></image><view class="add"><block wx:if="{{$root.m0}}"><image class="icon" src="../../static/icon-17.png" mode="widthFix" data-event-opts="{{[['tap',[['toMeetingPage',['$0','insert'],['NULL']]]]]}}" bindtap="__e"></image></block><text>创建会议</text></view><block wx:for="{{list}}" wx:for-item="one" wx:for-index="__i0__" wx:key="date"><view><view class="list-title">{{one.date}}</view><block wx:for="{{one.list}}" wx:for-item="meeting" wx:for-index="__i1__" wx:key="id"><view class="item"><view class="header"><view class="left"><block wx:if="{{meeting.type=='线上会议'}}"><image class="icon" src="../../static/icon-11.png" mode="widthFix"></image></block><block wx:if="{{meeting.type=='线下会议'}}"><image class="icon" src="../../static/icon-12.png" mode="widthFix"></image></block><text>{{meeting.type}}</text><text class="{{[meeting.status=='未开始'?'blue':'red']}}">{{"（"+meeting.status+"）"}}</text></view><view class="right"><text>编辑</text></view></view><view class="content"><view class="title">{{meeting.title}}</view><view class="attr"><view class="timer"><image class="icon" src="../../static/icon-14.png" mode="widthFix"></image><text>{{meeting.start+" ~ "+meeting.end}}</text></view><view class="creator"><image class="icon" src="../../static/icon-15.png" mode="widthFix"></image><text>{{meeting.name}}</text></view><block wx:if="{{meeting.type=='线下会议'}}"><view class="place"><image class="icon" src="../../static/icon-16.png" mode="widthFix"></image><text>{{meeting.place}}</text></view></block></view><view class="desc">{{meeting.desc}}</view><block wx:if="{{meeting.type=='线上会议'}}"><button class="btn">进入</button></block></view></view></block></view></block></view>