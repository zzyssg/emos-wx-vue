.page {
  padding: 35rpx 0;
}
.header {
  position: relative;
  padding: 0 35rpx;
}
.header .title {
  border-bottom: solid 1rpx #f0f0f0;
  padding: 30rpx 15rpx;
  font-size: 34rpx;
  color: #3474FF;
  font-weight: bold;
}
.header .title-placeholder {
  font-size: 34rpx;
}
.header .edit-icon {
  width: 40rpx;
  position: absolute;
  right: 45rpx;
  top: 35rpx;
}
.attr {
  padding: 0 35rpx;
  border-bottom: solid 20rpx #EEEEF4;
}
.attr .list .item {
  padding: 30rpx 10rpx 30rpx 15rpx;
  border-bottom: solid 1rpx #f0f0f0;
  font-size: 30rpx;
  color: #333;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.attr .list .item .value {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  color: #333;
}
.attr .desc {
  padding: 30rpx 15rpx 50rpx 15rpx;
  font-size: 30rpx;
  color: #333;
  display: block;
  line-height: 1.6;
}
.members {
  padding: 45rpx 25rpx;
}
.members .number {
  margin-bottom: 35rpx;
  color: #3474FF;
  font-size: 34rpx;
  padding-left: 20rpx;
  font-weight: bold;
}
.members .member {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.members .member .user {
  width: 100rpx;
  text-align: center;
  max-width: 20%;
  min-width: 20%;
  margin-bottom: 40rpx;
}
.members .member .user .photo {
  width: 100rpx;
  height: 100rpx;
  -webkit-border-radius: 50rpx;
          border-radius: 50rpx;
  margin-bottom: 5rpx;
}
.members .member .user .name {
  font-size: 30rpx;
  color: #333;
  display: block;
  text-align: center;
}
.members .member .add {
  width: 100rpx;
  max-width: 20%;
  min-width: 20%;
  margin-bottom: 40rpx;
}
.members .member .add .add-btn {
  width: 100rpx;
  display: block;
  margin-left: auto;
  margin-right: auto;
}
.btn {
  margin: 0 35rpx 35rpx 55rpx;
  background-color: #3474FF;
  color: #fff;
}
.btn:active {
  background-color: #0052FF;
}

